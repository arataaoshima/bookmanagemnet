<div class="container">
<h1>会員一覧</h1>

  <table class="table table-striped">
  <thead>
    <tr>
      <th>お名前</th>
      <th>Email</th>
      <th>パスワード</th>
      <th>電話</th>
      <th>住所</th>
      <th>アドミ権限</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  
   <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.password %></td>
        <td><%= user.phone %></td>
        <td><%= user.address %></td>
        <td><%= user.admin %></td>
        <td><%= link_to("Edit","/users/#{user.id}/edit") %></td>
    <% end %>
  </tbody>
</table>
</div>